<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    tools:context=".activity.RankingActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:orientation="vertical">
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="60dp">
            <ImageView
                android:id="@+id/ranking_back_tv"
                android:layout_marginLeft="20dp"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:src="@drawable/back_icon"
                android:layout_gravity="center_vertical"
                />
            <TextView
                android:id="@+id/ranking_store_tv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="center_horizontal"
                android:textSize="20sp"
                android:text="음식 랭킹"
                android:textColor="#323232"
                android:fontFamily="@font/note_medium"
                />
        </FrameLayout>

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@drawable/ranking_banner" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginBottom="20sp"
            android:gravity="center_vertical">

            <ImageView
                android:id="@+id/ranking_img_first"
                android:layout_width="50sp"
                android:layout_height="50sp"
                android:src="@drawable/ranking_first" />

            <TextView
                android:id="@+id/ranking_text_first"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="테스트입니다."
                android:layout_marginStart="20sp"
                android:fontFamily="@font/baemin_font"
                android:textSize="20sp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginBottom="20sp"
            android:gravity="center_vertical">

            <ImageView
                android:id="@+id/ranking_img_second"
                android:layout_width="50sp"
                android:layout_height="50sp"
                android:src="@drawable/ranking_second" />

            <TextView
                android:id="@+id/ranking_text_second"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="테스트입니다."
                android:layout_marginStart="20sp"
                android:fontFamily="@font/baemin_font"
                android:textSize="20sp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginBottom="20sp"
            android:gravity="center_vertical">

            <ImageView
                android:id="@+id/ranking_img_third"
                android:layout_width="50sp"
                android:layout_height="50sp"
                android:src="@drawable/ranking_third" />

            <TextView
                android:id="@+id/ranking_text_third"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="테스트입니다."
                android:layout_marginStart="20sp"
                android:fontFamily="@font/baemin_font"
                android:textSize="20sp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginBottom="20sp"
            android:gravity="center_vertical">

            <ImageView
                android:id="@+id/ranking_img_forth"
                android:layout_width="50sp"
                android:layout_height="50sp"
                android:src="@drawable/ranking_forth" />

            <TextView
                android:id="@+id/ranking_text_forth"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="테스트입니다."
                android:layout_marginStart="20sp"
                android:fontFamily="@font/baemin_font"
                android:textSize="20sp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginBottom="20sp"
            android:gravity="center_vertical">

            <ImageView
                android:id="@+id/ranking_img_fiveth"
                android:layout_width="50sp"
                android:layout_height="50sp"
                android:src="@drawable/ranking_fiveth" />

            <TextView
                android:id="@+id/ranking_text_fiveth"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="테스트입니다."
                android:layout_marginStart="20sp"
                android:fontFamily="@font/baemin_font"
                android:textSize="20sp"/>
        </LinearLayout>

    </LinearLayout>

</FrameLayout>